  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "bun --watch src/app/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun dist/app/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test --inspect-brk --inspect --logHeapUsage",
    "test:e2e": "bun test test/**/*.e2e-spec.ts",
    "test:jest": "bunx jest",
    "test:jest:watch": "bunx jest --watch",
    "test:jest:cov": "bunx jest --coverage",
    "test:jest:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:jest:e2e": "bunx jest --config ./test/jest-e2e.json"
  },
